
@model User
<div class="container">
    <div class="row">
        <div class="col-lg-6 register">
            <h1>Register</h1>
                <form asp-action="Register" method="post" class="form-group">
                    <p>
                        <p><span asp-validation-for="FirstName">  </span></p>
                        <label asp-for="FirstName"></label>
                        <input  asp-for="FirstName">
                    </p>
                    <p>
                        <p><span asp-validation-for="LastName">  </span></p>
                        <label asp-for="LastName"></label>
                        <input  asp-for="LastName" >
                    </p>
                    <p>
                        <p><span asp-validation-for="Email" >  </span></p>
                        <label asp-for="Email"></label>
                        <input  asp-for="Email">
                    </p>
                    <p>
                        <p><span asp-validation-for="Password" >  </span></p>
                        <label asp-for="Password"></label>
                        <input  asp-for="Password"  >
                    </p>
                    <p>
                        <p><span asp-validation-for="ConfirmPassword">  </span></p>
                        <label asp-for="ConfirmPassword"></label>
                        <input  asp-for="ConfirmPassword" >
                    </p>
                    <p>
                        <input  type="submit" >
                    </p>
                </form>
    
    </div> <!-- card.// -->
    <div class="col-lg-6">
        <partial name="_Login" model="new UserLogin()"></partial> 
</div>
 